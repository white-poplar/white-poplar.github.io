---
layout: nil
---

{
    "total":{% paginator.total_posts %},
    "rows":[
        {% for post in paginator.posts %}
          {"title":"{{ post.title }}", 
          "url":"{{ site.url }}{{ post.url }}", 
          "date":"{{ post.date | date:'%d - %m - %Y'}}", 
          }
        {% if forloop.last == false %},{% endif %}
        {% endfor %}
    ]
}